<%# Add the Facebook login link here %>
<% if session[:user_id] %>
  <h1><%= @user.name %></h1>
  <h2>Email: <%= @user.email %></h2>
  <h2>Facebook UID: <%= @user.uid %></h2>
  <img src="<%= @user.image %>">
  <h2><%= link_to "Logout", logout_path%></h2>
<% else %>
  <%= link_to('Log in with Facebook!', '/auth/facebook', method: :post) %>
<% end %> 